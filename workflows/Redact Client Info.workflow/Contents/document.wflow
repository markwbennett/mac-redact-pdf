				<key>ActionParameters</key>
				<dict>
					<key>COMMAND_STRING</key>
					<string>#!/bin/bash

# Redact Client Info Quick Action
# Uses Claude CLI to identify and redact sensitive information

SCRIPT_PATH="$HOME/github/mac-redact-pdf/redact.sh"

# Process each file
for f in "$@"; do
    if [ -f "$f" ]; then
        # Get file extension
        EXT="${f##*.}"
        EXT_LOWER=$(echo "$EXT" | tr '[:upper:]' '[:lower:]')

        if [ "$EXT_LOWER" = "pdf" ] || [ "$EXT_LOWER" = "docx" ]; then
            # Run the redaction script
            "$SCRIPT_PATH" "$f" 2>&amp;1

            if [ $? -eq 0 ]; then
                osascript -e "display notification \"Redacted: $(basename "$f")\" with title \"Redaction Complete\" sound name \"Glass\""
            else
                osascript -e "display notification \"Failed: $(basename "$f")\" with title \"Redaction Error\" sound name \"Basso\""
            fi
        fi
    fi
done
</string>
